	char a[15];
	char b[15];
    for (int i = 0; i < 12; i++) { /*<<<<< 9,5,10,21,3,pass*/
        a[i] = b[i];
        i++;
        a[i] = b[i];
        i++;
        a[i] = b[i];
    }
    a[12] = b[12];
    a[13] = b[13];
}
