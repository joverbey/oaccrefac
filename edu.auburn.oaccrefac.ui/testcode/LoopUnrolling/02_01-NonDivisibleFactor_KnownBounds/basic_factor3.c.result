	char a[15];
	int i = 0;
	for (; i < 14 + -2; i++) {/*<<<<< 8,5,9,21,3,pass*/
		a[i] = '\0';
		i++;
		/*<<<<< 8,5,9,21,3,pass*/
		a[i] = '\0';
		i++;
		/*<<<<< 8,5,9,21,3,pass*/
		a[i] = '\0';
	}/*<<<<< 8,5,9,21,3,pass*/
	a[i] = '\0';
	i++;
	/*<<<<< 8,5,9,21,3,pass*/
	a[i] = '\0';
	i++;

    return 0;
}
